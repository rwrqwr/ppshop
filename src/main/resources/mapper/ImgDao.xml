<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fff.dao.ImgDao">


    <select id="queryById" resultType="com.fff.entity.Img">
        SELECT img_id,img_path,img_name FROM img WHERE img_id = #{imgId};
    </select>
</mapper>