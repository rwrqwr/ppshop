<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fff.dao.UserDao">
    <insert id="insertUser" >
        INSERT INTO user_info
        (user_name,user_password,user_tel,user_email)
        VALUES (#{userName},#{userPassword},#{userTel},#{userEmail});
    </insert>
    <select id="queryByTel" resultType="com.fff.entity.User" >
      SELECT * FROM user_info WHERE user_tel = #{userTel};
    </select>
</mapper>